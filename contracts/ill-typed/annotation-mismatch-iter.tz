# Surprisingly this script is considered well-typed
# on alphanet

parameter (pair int (int %x));
storage (pair (int %z) int);
code { CAR ;
       PUSH (list int) {2; 3; };
       ITER { DROP; CAST (pair (int %y) int); };
       NIL operation;
       PAIR ;
      };
